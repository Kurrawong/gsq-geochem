geochem-shapes:DatasetMandatory
    a sh:NodeShape ;
    rdfs:label "Dataset's Mandatory Requirements" ;
    sh:property [
        a sh:PropertyShape ;
        sh:class tern:Distribution ;
        sh:description "An available distribution of the dataset." ;
        sh:name "dcat:distribution" ;
        sh:nodeKind sh:IRI ;
        sh:path dcat:distribution
    ] ;
.